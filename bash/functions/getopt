# getopt - wrapper module for agriffis' pure-getopt

# Location of this script
[[ "${BASH_SOURCE:0:1}" == "/" ]] && __self="$BASH_SOURCE" || __self="$PWD/$BASH_SOURCE"

# Source pure-getopt
source "${__self%/*}/third_party/agriffis/pure-getopt/getopt.bash"

# Prevent environment pollution
unset __self

# vim: ft=bash
